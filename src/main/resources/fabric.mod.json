{
  "schemaVersion": 1,
  "id": "nguhroutes",
  "version": "${version}",
  "name": "nguhroutes",
  "description": "Route planning for Cenrail and more",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/nguhroutes/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "org.nguhroutes.nguhroutes.client.NguhroutesDataGenerator"
    ],
    "client": [
      "org.nguhroutes.nguhroutes.client.NguhroutesClient"
    ],
    "main": [
      "org.nguhroutes.nguhroutes.Nguhroutes"
    ]
  },
  "mixins": [
